{
  "name": "aivestor",
  "version": "1.0.0",
  "scripts": {
    "start-backend": "cd backend && npm start",
    "start-frontend": "cd finai-assistant && npm start",
    "start-backend-dev": "cd backend && npm run dev",
    "start-backend-all": "cd backend && npm run start:all",
    "install-backend": "cd backend && npm install",
    "install-frontend": "cd finai-assistant && npm install",
    "install-all": "npm run install-backend && npm run install-frontend",
    "start": "npm run start-backend",
    "dev": "concurrently \"npm run start-backend-all\" \"npm run start-frontend\""
  },
  "dependencies": {
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "upstox-js-sdk": "^2.16.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
